{
  "name": "letterpresser",
  "version": "0.0.2",
  "description": "helping strategies for letterpress",
  "main": "./server/web.js",
  "engines": {
    "node": ">=6.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanmailander/letterpresser"
  },
  "keywords": [
    "node",
    "heroku",
    "letterpress"
  ],
  "author": "Sean Mailander",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run build",
    "build": "webpack",
    "watch": "webpack -w",
    "start": "node dist/server/js/server.js",
