    "test-watch": "mocha ./src/__tests__/index.js --require babel-core/register --watch"
  },
  "dependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "copy-webpack-plugin": "^4.0.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "express-validator": "^3.1.2",
    "express-livereload": "0.0.24",
    "express": "^4.14.1",
    "superagent": "^3.4.1",
    "superagent-bluebird-promise": "^4.1.0",
