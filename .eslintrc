{
  "extends": "airbnb",
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-curly-spacing": [2, "always"],
    "react/prefer-stateless-function": [0],
    "no-unused-vars": "warn",
    "max-len": "warn",
    "no-bitwise": "off"
  },
  "settings": {
    "import/resolver": "webpack"
    },
  "parser": "babel-eslint"
}